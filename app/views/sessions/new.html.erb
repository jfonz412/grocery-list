<% provide(:title, "Sign In") %>
<div class="jumbotron">
	<%= form_for :session, url: sessions_create_path, html: {class: "form-signin"} do |f| %>
			<%= f.label :email %>
			<%= f.text_field :email, class: 'form-control'%>
			<br>
			<%= f.label :password %>
			<%= f.text_field :password, class: 'form-control'%>
			<br>
			<%= f.submit "Sign in", class:"btn btn-lg btn-success" %>
	<% end %>
</div>
